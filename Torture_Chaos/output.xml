    <TextStringDefinition InstanceID="0xDDC95E93" TextString="折磨与混乱"/>
    <TextStringDefinition InstanceID="0xF3A92B63" TextString="{0.SimName} 度过了美好的一生，但{M0.他}{F0.她}的时间终于到了。{M0.他}{F0.她}正死于过热。"/>
    <TextStringDefinition InstanceID="0xB5EB5222" TextString="{0.SimName} 正死于心脏病发作！"/>
